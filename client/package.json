{
  "name": "pv-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "npm ci && vite build",
    "build:domain": "VITE_BASE_PATH=/safee/ vite build",
    "build:domain:win": "set VITE_BASE_PATH=/safee/ && vite build",
    "build:root": "VITE_BASE_PATH=/ vite build",
    "build:root:win": "set VITE_BASE_PATH=/ && vite build",
    "preview": "vite preview",
    "start": "npm run preview",
    "docker:build": "docker build -t vault-client .",
    "docker:run": "docker run -p 8080:8080 vault-client",
    "docker:dev": "docker build -f Dockerfile.dev -t vault-client-dev . && docker run -p 5173:5173 -v $(pwd)/src:/app/src vault-client-dev",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:unit": "vitest run --coverage",
    "test:ci": "node ../ci-test-runner.js || node ../ci-test-simple.js",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test:unit && npm run test:e2e",
    "playwright:install": "playwright install",
    "analyze": "vite build --mode analyze",
    "serve": "vite preview --port 4173 --host"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/react-router-dom": "^5.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.542.0",
    "pdfjs-dist": "^4.7.76",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.8.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.37.5",
    "happy-dom": "^18.0.1",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
