{
  "name": "personal-vault",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "bootstrap": "npm install",
    "dev:client": "npm --workspace client run dev",
    "dev:server": "npm --workspace server run dev",
    "build": "npm --workspace server run build",
    "build:full": "npm --workspace client run build && npm --workspace server run build",
    "start": "npm --workspace server run start"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "mongodb": "^6.19.0"
  }
}
